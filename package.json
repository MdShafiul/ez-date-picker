{
  "name": "ez-date-picker",
  "version": "0.1.0",
  "description": "A modern, lightweight React date picker component.",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "sideEffects": [
    "*.css"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./styles.css": "./dist/styles.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "if exist dist rmdir /s /q dist",
    "prepare": "npm run build"
  },
  "keywords": [
    "react",
    "datepicker",
    "date-picker",
    "ui-component",
    "calendar"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "tsup": "^8.2.4",
    "typescript": "^5.6.3"
  }
}
